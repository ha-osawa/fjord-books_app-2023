<%= link_to t('views.common.back', name: i18n_pluralize(Book.model_name.human.downcase)), books_path %>
<h1>Users</h1>

<div class="index-items">
  <% @users.each do |user| %>
    <div class="index-item">
      <%= render user %>
      <p> 
      <%= link_to 'show this user', user %> <br/>
      <%= link_to('edit my account', edit_user_registration_path) if user == current_user %> 
      </p>
    </div>
  <% end %>
</div>

<%= paginate @users%>
